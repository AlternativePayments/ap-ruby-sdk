<%= form_tag voids_path, :method => :get do %>
    <label>Insert transaction id: </label>
    <input id="transaction_id" name="transaction_id" type="text">

    <%= submit_tag('Retrieve') %>
<% end %>

<% if @voids.present? %>
    <% @voids.items.each_with_index do |void, index| %>
        <label><%= "#{index + 1}. #{void.to_json}" %></label>
        <br/>
    <% end %>
<% end %>